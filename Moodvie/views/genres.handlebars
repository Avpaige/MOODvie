<img class="moodvieLogo" src="../img/MOODVIEiconsandposter/MOODVIELOGO/MOODVie.jpg">
<div class="container-fluid">
    <p class="text-center text-light resultTitle">Way to go, you're a Whole MOOD!</p>

    <div id="results">
        <button id="closeBtn" type="button" class="close" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>

    <div id="pageBtns">
        <a href="#" class="previous round">&#8249;</a>
        <a href="#" class="next round">&#8250;</a>
    </div>

    <div id="genreDiv">
        {{#each genresData}}
        <div class="genres">
            {{genre}}
            <br>
            <img src="{{img}}">
            <a href="/genres/{{genre}}">Click to see movies in this genre!</a>
        </div>
        {{/each}}
    </div>

    <script src="https://code.jquery.com/jquery-3.4.1.min.js">
